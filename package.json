{
  "name": "project-hive",
  "version": "1.0.0",
  "notes": "NOTE: This version of the project is for development and will not run properly on the pi.",
  "description": "A raspberry pi running a restful API. Includes a database to manage user data including events and hashed access codes.",
  "main": "./src/app.js",
  "dependencies": {
    "aws-sdk": "^2.329.0",
    "aws-sdk-mock": "^4.2.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "http-errors": "^1.7.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "multer": "^1.4.0",
    "popmotion": "^8.5.0",
    "react-pose": "^3.4.0",
    "react-scripts": "^2.1.1",
    "superagent": "^4.0.0-beta.5",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "artillery": "^1.6.0-25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "superagent": "^4.0.0-beta.5",
    "uuid": "^3.3.2",
    "validator": "^10.8.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "artillery": "./node_modules/artillery/bin/artillery $1",
    "start": "node back-end/src/app.js",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "test": "eslint . && jest --coverage --collectCoverageFrom=src/lib/*.js --collectCoverageFrom=src/routes/*.js --collectCoverageFrom=src/__test__/lib/*-mock.js --collectCoverageFrom=!src/lib/error-middleware.js --forceExit --runInBand",
    "jestSF": "./node_modules/jest/bin/jest.js --coverage --forceExit --runInBand $1",
    "justJest": "jest --coverage --forceExit --runInBand",
    "justLint": "./node_modules/eslint/bin/eslint.js ./src/*.js",
    "start-server": "./makesure.sh; node ./src/app.js",
    "dbDevOn": "mkdir -p ./testdb && mongod --dbpath ./testdb",
    "devDbOff": "./killDevDb.sh",
    "dbOn": "mkdir -p ./db && mongod --dbpath ./db",
    "dbOff": "killall mongod",
    "signup": "./presentation/test-hive.sh signup",
    "arm": "./presentation/test-hive.sh arm",
    "disarm": "./presentation/test-hive.sh disarm",
    "more-signups": "./presentation/test-hive.sh more-signups",
    "react-scripts start": "react-scripts start",
    "react-scripts build": "react-scripts build",
    "react-scripts test": "react-scripts test --env=jsdom",
    "react-scripts eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgwest/project-hive.git"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__test__/lib/test.env.js"
    ],
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/src/__test__/mock-asset.js"
    }
  },
  "keywords": [],
